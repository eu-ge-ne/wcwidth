{
  "name": "@eu-ge-ne/wcwidth",
  "version": "0.0.2",
  "license": "MIT",
  "exports": "./src/mod.ts",
  "imports": {
    "@std/assert": "jsr:@std/assert@^1.0.13"
  },
  "publish": {
    "exclude": [
      ".github",
      ".vscode",
      "bench",
      "test",
      "codecov.yml"
    ]
  },
  "compilerOptions": {
    "noUncheckedIndexedAccess": true
  },
  "lint": {
    "rules": {
      "tags": ["recommended", "jsr"],
      "include": [
        "eqeqeq",
        "explicit-module-boundary-types",
        "explicit-function-return-type",
        "no-console",
        "no-eval",
        "no-external-import"
      ]
    }
  }
}
